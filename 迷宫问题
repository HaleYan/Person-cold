#include<iostream>
#include<stdio.h>
/*aaaaaaaaaaaa
aUDDLUULRULa
aUURLLLRRRUa
aRRUURLDLRDa
aRUDDDDUUUUa
aURUDLLRRUUa
aDURLRLDLRLa
aULLURLLRDUa
aRDLULLRDDDa
aUUDDUDUDLLa
aULRDLUURRRa
aaaaaaaaaaaa*/
using namespace std;
int main(){
	char ch[12][12],chars;
	for(int i=0;i<12;i++)
	cin>>ch[i]; 
	int sum=0,m=0;
	for(int i=1;i<=10;i++)
		for(int j=1;j<=10;j++){
			chars=ch[i][j];
			while(chars!='a'){
			if(chars=='L') chars=ch[i-1][j];
			if(chars=='R') chars=ch[i+1][j];
			if(chars=='U') chars=ch[i][j+1];
			if(chars=='D') chars=ch[i][j-1];
			m++;
			if(m>20) break;
		}
		if(chars=='a') sum++;
	}
	cout<<sum<<endl;
	return 0;
} 
